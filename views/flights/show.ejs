<%- include('../partials/header') %>

<% if (flights.destination.length) { %>
  <h2> <%= title %> </h2>
  <table>
    <thead>
      <tr>
        <th>Date</th>
        <th>Airport</th>
        <th></th>
      </tr>
    </thead>
    <tbody>
      <% destinations.forEach(function(r) { %>
        <tr>
          <td> <%= r.arrival.toLocaleDateString() %> </td>
          <td> <%= r.aiport %> </td>
        
        </tr>
      <% }); %>
    </tbody>
  </table>
<% } else { %>
  <h5>No Reviews Yet</h5>
<% } %>

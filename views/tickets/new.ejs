<%- include('../partials/header') %>
<h2><%= title %></h2>
<img id="bookpic" src="/images/boardpass.png"></img>
<form id="ticket-form" action="/flights/<%= flightId %>/tickets" method=POST>
  <label>Seat
      <input id="add-ticket" name="seat" type="text" required pattern="[A-F][1-9]\d?">
  </label>    
  <label>Price
      <input name="price" type="text">
  </label>
  <input type="submit" value="Add Ticket">
</form>

<%- include('../partials/footer') %>